                AnimationTrack = Humanoid:LoadAnimation(Animation)
                AnimationTrack:Play()
            else
                -- Stop animation
                if AnimationTrack then
                    AnimationTrack:Stop()
                    AnimationTrack:Destroy()
                    AnimationTrack = nil
                end
                if Animation then
                    Animation:Destroy()
                    Animation = nil
                end
            end
        end
    end,
})

